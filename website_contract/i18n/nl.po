msgstr "\n            \n    <div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: rgb(255, 255, 255); \">\n\n        <p>Hallo ${object.partner_id.name},</p>\n        \n        <p>\n            Uw abonnement (${ctx.get('code')}) is zojuist vernieuwd\n            %if ctx.get('total_amount') and ctx.get('payment_method'):\n                via de betaling ${ctx['total_amount']} ${ctx['currency']} geheven op ${ctx['payment_method']}\n                .\n            %endif\n        </p>\n\n        <p>Uw factuur is bijgesloten</p>\n\n        %if ctx.get('next_date'):\n        <p>\n          Uw volgende factuur zal zijn op ${ctx.get('next_date')}.\n        </p>\n        %endif\n\n        <br/>\n        <p>Als u vragen heeft, neem met ons contact op.</p>\n        <p>Bedankt voor het kiezen van ${object.company_id.name or 'us'}!</p>\n        <br/>\n        <br/>\n        <div style=\"width: 375px; margin: 0px; padding: 0px; background-color: #8E0000; border-top-left-radius: 5px 5px; border-top-right-radius: 5px 5px; background-repeat: repeat no-repeat;\">\n            <h3 style=\"margin: 0px; padding: 2px 14px; font-size: 12px; color: #DDD;\">\n                <strong style=\"text-transform:uppercase;\">${object.company_id.name}</strong></h3>\n        </div>\n        <div style=\"width: 347px; margin: 0px; padding: 5px 14px; line-height: 16px; background-color: #F2F2F2;\">\n            <span style=\"color: #222; margin-bottom: 5px; display: block; \">\n                ${object.company_id.partner_id.sudo().with_context(show_address=True, html_format=True).name_get()[0][1] | safe}\n            </span>\n            % if object.company_id.phone:\n                <div style=\"margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; padding-left: 0px; \">\n                    Phone:&nbsp; ${object.company_id.phone}\n                </div>\n            % endif\n            % if object.company_id.website:\n                <div>\n                    Web :&nbsp;<a href=\"${object.company_id.website}\">${object.company_id.website}</a>\n                </div>\n            % endif\n            <p></p>\n        </div>\n    </div>\n                \n        "

#. module: website_contract
#: model:mail.template,body_html:website_contract.email_payment_close
msgid ""
"\n"
"            \n"
"    <div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: rgb(255, 255, 255); \">\n"
"\n"
"        <p>Hello ${object.partner_id.name},</p>\n"
"        \n"
"        <p>\n"
"        % if ctx.get('payment_method') and ctx.get('total_amount'):\n"
"          Our final attempt to process a payment for your subscription using your payment method\n"
"          ${ctx['payment_method']}\n"
"          for ${ctx['total_amount']} ${ctx.get('currency')} failed.\n"
"          % if ctx.get('error'):\n"
"            Your bank or credit institution gave the following details about the issue: <pre>${ctx['error']}</pre>.\n"
"          %endif\n"
"        % else:\n"
"          Our final attempt to process a payment for your subscription failed because we have no payment method recorded for you.\n"
"        % endif \n"
"        </p>\n"
"\n"
"        <p>\n"
"          As your payment should have been made <strong>15 days ago</strong>, your subscription has been terminated.\n"
"        </p>\n"
"\n"
"        <br/>\n"
"        <p>Should you wish to resolve this issue, do not hesitate to contact us.</p>\n"
"        <p>Thank you for choosing ${object.company_id.name or 'us'}!</p>\n"
"        <br/>\n"
"        <br/>\n"
"        <div style=\"width: 375px; margin: 0px; padding: 0px; background-color: #8E0000; border-top-left-radius: 5px 5px; border-top-right-radius: 5px 5px; background-repeat: repeat no-repeat;\">\n"
"            <h3 style=\"margin: 0px; padding: 2px 14px; font-size: 12px; color: #DDD;\">\n"
"                <strong style=\"text-transform:uppercase;\">${object.company_id.name}</strong></h3>\n"
"        </div>\n"
"        <div style=\"width: 347px; margin: 0px; padding: 5px 14px; line-height: 16px; background-color: #F2F2F2;\">\n"
"            <span style=\"color: #222; margin-bottom: 5px; display: block; \">\n"
"                ${object.company_id.partner_id.sudo().with_context(show_address=True, html_format=True).name_get()[0][1] | safe}\n"
"            </span>\n"
"            % if object.company_id.phone:\n"
"                <div style=\"margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; padding-left: 0px; \">\n"
"                    Phone:&nbsp; ${object.company_id.phone}\n"
"                </div>\n"
"            % endif\n"
"            % if object.company_id.website:\n"
"                <div>\n"
"                    Web :&nbsp;<a href=\"${object.company_id.website}\">${object.company_id.website}</a>\n"
"                </div>\n"
"            % endif\n"
"            <p></p>\n"
"        </div>\n"
"    </div>\n"
"                \n"
"        "
msgstr "\n            \n    <div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: rgb(255, 255, 255); \">\n\n        <p>Hallo ${object.partner_id.name},</p>\n        \n        <p>\n        % if ctx.get('payment_method') and ctx.get('total_amount'):\n          Dis is onze laatste poging om de betaling van je inschrijving te verwerken met de door u gekozen betaalmethode \n          ${ctx['payment_method']}\n          for ${ctx['total_amount']} ${ctx.get('currency')} failed.\n          % if ctx.get('error'):\n            Uw bank of krediet verstrekker hebben u de volgende gegevens gegeven over het probleem : <pre>${ctx['error']}</pre>.\n          %endif\n        % else:\n          Onze laatste poging de betaling te verwerken is mislukt omdat we geen betaalwijze hebben ontvangen.\n        % endif \n        </p>\n\n        <p>\n          Omdat uw betaling <strong>15 dagen geleden</strong> gemaakt had moeten zijn, is uw inschrijving opgezegt.\n        </p>\n\n        <br/>\n        <p>Wilt u het probleem herstellen, aarzel dan niet om contact met ons op te nemen.</p>\n        <p>Dank u voor uw keuze in ${object.company_id.name or 'us'}!</p>\n        <br/>\n        <br/>\n        <div style=\"width: 375px; margin: 0px; padding: 0px; background-color: #8E0000; border-top-left-radius: 5px 5px; border-top-right-radius: 5px 5px; background-repeat: repeat no-repeat;\">\n            <h3 style=\"margin: 0px; padding: 2px 14px; font-size: 12px; color: #DDD;\">\n                <strong style=\"text-transform:uppercase;\">${object.company_id.name}</strong></h3>\n        </div>\n        <div style=\"width: 347px; margin: 0px; padding: 5px 14px; line-height: 16px; background-color: #F2F2F2;\">\n            <span style=\"color: #222; margin-bottom: 5px; display: block; \">\n                ${object.company_id.partner_id.sudo().with_context(show_address=True, html_format=True).name_get()[0][1] | safe}\n            </span>\n            % if object.company_id.phone:\n                <div style=\"margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; padding-left: 0px; \">\n                    Tel: &nbsp; ${object.company_id.phone}\n                </div>\n            % endif\n            % if object.company_id.website:\n                <div>\n                    Website: &nbsp;<a href=\"${object.company_id.website}\">${object.company_id.website}</a>\n                </div>\n            % endif\n            <p></p>\n        </div>\n    </div>\n                \n        "

#. module: website_contract
#: model:mail.template,body_html:website_contract.email_payment_reminder
msgid ""
"\n"
"            \n"
"    <div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: rgb(255, 255, 255); \">\n"
"\n"
"        <p>Hello ${object.partner_id.name},</p>\n"
"        \n"
"        <p>\n"
"        % if ctx.get('payment_method') and ctx.get('total_amount'):\n"
"          We were unable to process a payment for your subscription using your payment method\n"
"          ${ctx['payment_method']}\n"
"          for ${ctx['total_amount']} ${ctx.get('currency')}.\n"
"          % if ctx.get('error'):\n"
"            Your bank or credit institution gave the following details about the issue: <pre>${ctx['error']}</pre>.\n"
"          %endif\n"
"        % else:\n"
"          We were unable to process a payment for your subscription because we have no payment method recorded for you.\n"
"        % endif \n"
"        </p>\n"
"\n"
"        <p>\n"
"          Your subscription ${ctx.get('code')} is still valid but will be <b>suspended</b>\n"
"          on ${ctx.get('date_close')} unless the payment succeeds in the mean time (we will retry once every day).\n"
"          Please double-check that you have sufficient funds.\n"
"        </p>\n"
"\n"
"        <br/>\n"
"        <p>If you have any question, do not hesitate to contact us.</p>\n"
"        <p>Thank you for choosing ${object.company_id.name or 'us'}!</p>\n"
"        <br/>\n"
"        <br/>\n"
"        <div style=\"width: 375px; margin: 0px; padding: 0px; background-color: #8E0000; border-top-left-radius: 5px 5px; border-top-right-radius: 5px 5px; background-repeat: repeat no-repeat;\">\n"
"            <h3 style=\"margin: 0px; padding: 2px 14px; font-size: 12px; color: #DDD;\">\n"
"                <strong style=\"text-transform:uppercase;\">${object.company_id.name}</strong></h3>\n"
"        </div>\n"
"        <div style=\"width: 347px; margin: 0px; padding: 5px 14px; line-height: 16px; background-color: #F2F2F2;\">\n"
"            <span style=\"color: #222; margin-bottom: 5px; display: block; \">\n"
"                ${object.company_id.partner_id.sudo().with_context(show_address=True, html_format=True).name_get()[0][1] | safe}\n"
"            </span>\n"
"            % if object.company_id.phone:\n"
"                <div style=\"margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; padding-left: 0px; \">\n"
"                    Phone:&nbsp; ${object.company_id.phone}\n"
"                </div>\n"
"            % endif\n"
"            % if object.company_id.website:\n"
"                <div>\n"
"                    Web :&nbsp;<a href=\"${object.company_id.website}\">${object.company_id.website}</a>\n"
"                </div>\n"
"            % endif\n"
"            <p></p>\n"
"        </div>\n"
"    </div>\n"
"                \n"
"        "
msgstr "\n            \n    <div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: rgb(255, 255, 255); \">\n\n        <p>Hallo ${object.partner_id.name},</p>\n        \n        <p>\n        % if ctx.get('payment_method') and ctx.get('total_amount'):\n          We zijn niet in staat de betaling te verwerken met deze betaalwijze.\n          ${ctx['payment_method']}\n          for ${ctx['total_amount']} ${ctx.get('currency')}.\n          % if ctx.get('error'):\n            Uw bank of krediet verstrekker hebben u de volgende gegevens gegeven over het probleem : <pre>${ctx['error']}</pre>.\n          %endif\n        % else:\n          Onze poging de betaling te verwerken is mislukt omdat we geen betaalwijze hebben ontvangen\n        % endif \n        </p>\n\n        <p>\n          Uw inschrijving ${ctx.get('code')} is nog steeds actief maar wordt <b>geschorts</b>\n          op ${ctx.get('date_close')} behalve als u de betaling tussentijds wel is gelukt (we proberen het 1x per dag).\n          Controleer graag nogmaals of u voldoende saldo heeft\n        </p>\n\n        <br/>\n        <p>Als u vragen heeft, aarzel niet om contact met ons op te nemen.</p>\n        <p>Bedankt voor het kiezen van ${object.company_id.name or 'us'}!</p>\n        <br/>\n        <br/>\n        <div style=\"width: 375px; margin: 0px; padding: 0px; background-color: #8E0000; border-top-left-radius: 5px 5px; border-top-right-radius: 5px 5px; background-repeat: repeat no-repeat;\">\n            <h3 style=\"margin: 0px; padding: 2px 14px; font-size: 12px; color: #DDD;\">\n                <strong style=\"text-transform:uppercase;\">${object.company_id.name}</strong></h3>\n        </div>\n        <div style=\"width: 347px; margin: 0px; padding: 5px 14px; line-height: 16px; background-color: #F2F2F2;\">\n            <span style=\"color: #222; margin-bottom: 5px; display: block; \">\n                ${object.company_id.partner_id.sudo().with_context(show_address=True, html_format=True).name_get()[0][1] | safe}\n            </span>\n            % if object.company_id.phone:\n                <div style=\"margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; padding-left: 0px; \">\n                    Tel: &nbsp; ${object.company_id.phone}\n                </div>\n            % endif\n            % if object.company_id.website:\n                <div>\n                    Website: &nbsp;<a href=\"${object.company_id.website}\">${object.company_id.website}</a>\n                </div>\n            % endif\n            <p></p>\n        </div>\n    </div>\n                \n        "

