#. module: crm_voip
#: model:mail.template,body_html:crm_voip.email_template_call_client_notAvailable
msgid ""
"\n"
"                <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"\n"
"                    <p>Hello ${object.name},</p>\n"
"\n"
"                    <p>I just tried to call you.\n"
"                        Can you please give me your availability in order to schedule a new call?</p>\n"
"                    <p>Yours Sincerly</p>\n"
"                    <p>${user.name}</p>\n"
"\n"
"                </div>\n"
"            "
msgstr "\n<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \"><p>Hola ${object.name},</p><p>Justo he intentado llamarte. ¿Puedes enviarme tu disponibilidad para planificar una nueva llamada?</p><p>Saluda,</p><p>${user.name}</p></div>"

#. module: crm_voip
#: model:mail.template,body_html:crm_voip.email_template_call_interrupted
msgid ""
"\n"
"                <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"                    <p>Hello ${object.name},</p>\n"
"\n"
"                    <p>It seems that we have been interrupted during our conversation. Can you please give me your availability in order to schedule \n"
"                        a new call? </p>\n"
"                    <p>Yours Sincerly</p>\n"
"                    <p>${user.name}</p>\n"
"\n"
"                </div>\n"
"            "
msgstr "\n<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \"><p>Hola ${object.name},</p><p>Parece que nos han interrumpido durante nuestra conversación. ¿Puedes enviarme tu disponibilidad para planificar una nueva llamada? </p> <p>Saluda,</p><p>${user.name}</p></div>"

